public class Nums {
    public static void main(String[] args){
        int[][] nums = {
            {1, 2},
            {3, 4, 5}

        };

        System.out.println(nums[0][0]);
        System.out.println(nums[0][1]);
        System.out.println(nums[1][0]);
        System.out.println(nums[1][1]);
        System.out.println(nums[1][2]);

    }

}
